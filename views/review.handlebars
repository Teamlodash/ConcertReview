<div class="container" id="fm-api">
  <div class="row">
    <div class="col-md-12">

      <div class="panel panel-primary" id="bioDiv">
        <div class="panel-heading">Bio: </div>
        <div class="panel-body" id="artist-div"></div>
      </div>

    </div>
  </div>
</div>



<div class="container">
  <div class="row">

    <div class="col-lg-12">

      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title"><strong>Artist Reviews</strong></h3>
        </div>
        <div class="panel-body">
          {{#each x}}

          <div class="panel panel-default">

            <div class="panel-heading"> Artist: {{Artist}} | Date: {{DateOfConcert}} | Venue: {{Venue}}
            </div>

            <div class="panel-body"> {{Review}}
            </div>
          </div>

          {{/each}}
        </div>
      </div>

    </div>
  </div>
</div>

